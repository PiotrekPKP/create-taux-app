{
  "name": "create-taux-app",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/cli",
    "apps/docs"
  ],
  "scripts": {
    "cli": "cd apps/cli && pnpm cli",
    "build": "turbo run build --filter=!./apps/cli/template",
    "dev": "turbo run dev --filter=!./apps/cli/template",
    "dev:template": "turbo run dev --filter=./apps/cli/template",
    "lint": "turbo run lint",
    "pub": "cd apps/cli && pnpm pub"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "^1.8.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.1.0"
}
